<div class="row blc-docs">
  <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8 col-xl-8 list-docs">
    each row_docs in all_docs
      <div class="custom-control custom-switch">
        <br>
        <input type="checkbox" value="#{row_docs.name}" class="custom-control-input" id="status">
        <label class="custom-control-label" for="status">#{row_docs.name}</label>
      </div>
  </div>
</div>